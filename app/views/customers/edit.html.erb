 <body>
    <div class="container">
      <div class="col-xs-offset-1 col-xs-11"><br><br><br>
        <h3>登録情報編集</h3>
      </div>
    </div>
          <%= form_with model: @user, local:true do |f| %>
            <div class="field form-group col-xs-12">
              <%= f.label :名前　　　　　　　（姓） %>
              <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name" %>
              <%= f.label :（名）　 %>
              <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name" %>
            </div>
            <div class="field form-group col-xs-12">
              <%= f.label :フリガナ　　　　（セイ） %>
              <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "first_name_kana" %>
              <%= f.label :（メイ） %>
              <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "last_name_kana" %>
            </div>
            <div class="field form-group col-xs-12">
              <%= f.label :メールアドレス　　　　　 %>
              <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
            </div>
            <div class="field form-group col-xs-12">
              <%= f.label :郵便番号（ハイフンなし） %>
              <%= f.text_field :postal_code, autofocus: true, autocomplete: "postal_code" %>
            </div>
             <div class="field form-group col-xs-12">
              <%= f.label :住所　　　　　　　　　　 %>
              <%= f.text_field :address, autofocus: true, autocomplete: "address" %>
            </div>
            <div class="field form-group col-xs-12">
              <%= f.label :電話番号（ハイフンなし） %>
              <%= f.text_field :telephone_number, autofocus: true, autocomplete: "telephone_number" %>
            </div>
             <div class="container">
               <th><%= f.submit '編集内容を保存する',class: "btn btn-primary" %></th>
               <th><%= f.submit '退会する',class: "btn btn-danger",　method: :delete %></th>
              </div>
          <% end %>
        </div>
      </div>
    </div>
  </body>
