  <html>
    <body>
      <div class="container"><br><br><br><br><br>
        <table>
          <tr>
            <td><h3><%= @customer.first_name %><%= @customer.last_name %>さんの会員詳細</h3></td>
          <tr>
          </table>
            <table class="table table-bordered">
            <tr>
              <th>会員ID</th>
              <th><%= @customer.id %></th>
            <tr>
              <th>氏名</th>
              <th><%= @customer.first_name %><%= @customer.last_name %></th>
            </tr>
              <th>フリガナ</th>
              <th><%= @customer.first_name_kana %><%= @customer.last_name_kana %></th>
            </tr>
            <tr>
              <th>郵便番号</th>
              <th><%= @customer.postal_code %></th>
            </tr>
            <tr>
              <th>住所</th>
              <th><%= @customer.address %></th>
            </tr>
            </tr>
              <th>電話番号</th>
              <th><%= @customer.telephone_number %></th>
            </tr>
            <tr>
              <th>メールアドレス</th>
              <th><%= @customer.email %></th>
            </tr>
            </table>
          <tr>
             <th><%= link_to "編集する", edit_admins_end_user_path, class: 'btn btn-primary' %></th>
          </tr>
          <tr>
             <th><%= link_to "注文履歴を見る", class: 'btn btn-primary' %></th>
          </tr>
        </table>
      </div>
    </body>
  </html>